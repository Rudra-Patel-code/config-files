    // My Config

    "tray": {
        "icon-size": {},
        "spacing": 5
    },

    "hyprland/window": {
        "format": " ÔåÉ {}",
        "separate-outputs": true,
        "rewrite": {
            "${USER}@${set_sysname}:(.*)": "$1 Ôíâ",
            "(.*) ‚Äî Mozilla Firefox": "$1 Û∞àπ",
            "(.*)Mozilla Firefox": "Firefox Û∞àπ",
            "(.*) - Visual Studio Code": "$1 Û∞®û",
            "(.*)Visual Studio Code": "Code Û∞®û",
            "(.*) ‚Äî Dolphin": "$1 Û∞âã",
            "(.*)Spotify": "Spotify Û∞ìá",
            "(.*)Steam": "Steam Û∞ìì"
        },
        "max-length": 1000
    },
    "hyprland/workspaces": {
        // "on-scroll-up": "hyprctl dispatch workspace -1",
        // "on-scroll-down": "hyprctl dispatch workspace +1",
        // "all-outputs": true,
        // "active-only": false,
        "format": "{name}",
        "format-icons": ["ÔëÑ", "ÔëÑ", "ÔëÑ"],
        "on-click": "activate",
        "persistent-workspaces": {
            // START WORKSPACES
            "*": 3
            // END WORKSPACES
        }
    },
    "custom/appmenu": {
        "format": "Apps",
        "on-click": "rofi -show drun -replace",
        "on-click-right": "jgmenu_run",
        //  "on-click-right": "~/dotfiles/hypr/scripts/keybindings.sh",
        "tooltip": false
    },
    "custom/browser": {
        "format": "Ôâ© ",
        "on-click": "firefox",
        "tooltip": false
    },
    "custom/terminal": {
        "format": "Ôíâ ",
        "on-click": "alacritty",
        "tooltip": false
    },
    "custom/filemanager": {
        "format": "ÔÅº ",
        "on-click": "dolphin",
        "tooltip": false
    },
    "group/quicklinks": {
        "orientation": "horizontal",
        "modules": [
            "custom/filemanager",
            "custom/browser",
            "custom/terminal"
            //          "custom/editor",
            // "custom/obsidian"
        ]
    },
    "wlr/taskbar": {
        "format": "{icon}",
        "icon-size": {},
        "icon-theme": "${i_theme}",
        "spacing": 0,
        "tooltip-format": "{title}",
        "on-click": "activate",
        "on-click-middle": "close",
        "on-click-right": "fullscreen",
        "ignore-list": [
            "com.github.hluk.copyq",
            "alacritty-dropterm",
            "feathernotes",
            "ranger",
            "org.keepassxc.KeePassXC"
        ],
        "app_ids-mapping": {
            "firefoxdeveloperedition": "firefox-developer-edition"
        }
    },
    "clock": {
        "format": "{:%R}",

        "tooltip-format": "<tt>{calendar}</tt>",
        "calendar": {
            "mode": "year",
            "mode-mon-col": 3,
            "on-scroll": 1,
            "on-click-right": "mode",
            "format": {
                "months": "<span color='#A5949B'><b>{}</b></span>",
                "weekdays": "<span color='#D9A99C'><b>{}</b></span>",
                "today": "<span color='#A5949B'><b>{}</b></span>"
            }
        },
        "actions": {
            "on-click-right": "mode",
            "on-click-forward": "tz_up",
            "on-click-backward": "tz_down",
            "on-scroll-up": "shift_up",
            "on-scroll-down": "shift_down"
        }
    },
    "custom/misc": {
        "format": "ÓàÄ ",
        "tooltip": false
    },
    "bluetooth": {
        "format-off": "Ôäì  {status}", // Bluetooth off icon
        "format-on": "Ôäì {status}", // Bluetooth on icon
        "format-connected": "Ôäì {device_alias} ({device_battery_percentage}%)",
        "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
        "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
        "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%",
        "on-click": "blueman-manager", // Turn on Bluetooth and open Blueman Manager
        "on-click-right": "blueman-manager", // Turn off Bluetooth
        "tooltip": true
    },

    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-muted": "Ôë¶",
        "on-click": "~/.config/Scripts/waybar/vol-progress.sh --toggle",
        "on-click-right": "pavucontrol",
        "on-scroll-up": "~/.config/Scripts/waybar/vol-progress.sh --inc",
        "on-scroll-down": "~/.config/Scripts/waybar/vol-progress.sh --dec",
        "tooltip-format": "{icon} {desc}",
        "format-icons": {
            "headphone": ["ÔÄ¶", "ÔÄß", "ÔÄ®"],
            "hands-free": "ÔÄ•",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        }
    },

    "pulseaudio#microphone": {
        "format": "{format_source}",
        "format-source": "Û∞ç¨",
        "format-source-muted": "ÔÑ±",
        "on-click": "pavucontrol -t 4",
        "on-click-middle": "~/.config/hypr/scripts/volumecontrol.sh -i m",
        "on-scroll-up": "~/.config/hypr/scripts/volumecontrol.sh -i i",
        "on-scroll-down": "~/.config/hypr/scripts/volumecontrol.sh -i d",
        "tooltip-format": "{format_source} {source_desc} // {source_volume}%",
        "scroll-step": 5
    },
    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format-wifi": "Û∞§® {signalStrength}%",
        "format-ethernet": "Û±òñ Wired",
        "tooltip-format": "Û±òñ {ipaddr} ÔÑπ {bandwidthUpBytes} ÔÑ∫ {bandwidthDownBytes}",
        "format-linked": "Û±òñ {ifname} (No IP)",
        "format-disconnected": "ÔÅ± Disconnected",
        "format-alt": "Û∞§® {essid}",
        "on-click": "alacritty --class nmtui -e nmtui",
        "interval": 5
    },
    "custom/ddc_brightness": {
        "format": "{icon} {percentage}%",
        "format-icons": ["ÔÅÇ"],
        "exec": "/home/rudra/.config/Scripts/waybar/waybar-ddc-module.sh",
        "return-type": "json",
        "on-scroll-up": "echo '+' > /tmp/waybar-ddc-module-rx",
        "on-scroll-down": "echo '-' > /tmp/waybar-ddc-module-rx",
        "on-click": "echo 'min' > /tmp/waybar-ddc-module-rx",
        "on-click-right": "echo 'max' > /tmp/waybar-ddc-module-rx",
        "tooltip": false
    },
    "battery": {
        "states": {
            "warning": 30,
            "critical": 15
        },
        "format": "{capacity}% {icon}",
        "format-full": "{capacity}% {icon}",
        "format-charging": "{capacity}% Ôá¶",
        "format-plugged": "{capacity}% Ôá¶",
        "format-alt": "{time} {icon}",
        "format-icons": ["Û∞Çé", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞Åπ"]
    },
    "backlight": {
        "format": "{percent}% {icon}",
        "format-icons": ["Óéç", "Óèì", "Óèë", "Óèè", "Óèé", "Óèç", "Óèä", "Óèà", "Óéõ"]
    },
    "mpd": {
        "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon}{singleIcon}{artist} - {album} - {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S}) ‚∏®{songPosition}|{queueLength}‚∏© {volume}% ÔÄÅ",
        "format-disconnected": "Disconnected ÔÄÅ",
        "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon}{singleIcon}Stopped ÔÄÅ",
        "unknown-tag": "N/A",
        "interval": 5,
        "consume-icons": {
            "on": "ÔÉÑ "
        },
        "random-icons": {
            "off": "<span color=\"#f53c3c\">ÔÅ¥</span> ",
            "on": "ÔÅ¥ "
        },
        "repeat-icons": {
            "on": "ÔÄû "
        },
        "single-icons": {
            "on": "ÔÄû1 "
        },
        "state-icons": {
            "paused": "ÔÅå",
            "playing": "ÔÅã"
        },
        "tooltip-format": "MPD (connected)",
        "tooltip-format-disconnected": "MPD (disconnected)"
    },
    "group/devices": {
        "orientation": "horizontal",
        "modules": [
            "mpd",
            "pulseaudio",
            "battery",
            "bluetooth",
            //TODO: ddc
            "backlight",
            "network"
        ]
    },

    //TODO: MAKO
    "custom/updates": {
        "format": "ÔÇ´  {}",
        "exec": "~/.config/hypr/scripts/updates.sh",
        "restart-interval": 60,
        "return-type": "json",
        "escape": true,
        "on-click": "~/.config/hypr/scripts/updates.sh up",
        "tooltip-format": "{}",
        "tooltip": true
    },
    "custom/mako": {
        "exec": "~/.config/Scripts/mako/mako.sh",
        "on-click": "~/.config/Scripts/mako/mako.sh toggle_mode",
        "interval": 1,
        "on-scroll-up": "makoctl dismiss",
        "on-scroll-down": "makoctl restore"
    },
    "custom/copyq": {
        "format": "Û∞Öç",
        "on-click": "copyq toggle",
        "tooltip": false
    },
    "custom/power": {
        "format": "ÔÄë{}",
        "exec": "echo ; echo ÔÄë logout",
        "on-click": "~/.config/hypr/scripts/logoutlaunch.sh 2",
        "interval": 86400, // once every day
        "tooltip": true
    },

    "group/system": {
        "orientation": "horizontal",
        "modules": [
            "custom/updates",
            "custom/mako",
            "custom/copyq",
            "custom/power"
        ]
    }
}
